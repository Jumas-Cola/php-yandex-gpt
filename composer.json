    "keywords": [
        "Yandex GPT",
        "yandex"
    ],
    "homepage": "https://github.com/TeaRiot/php-yandex-gpt",
    "license": "MIT",
    "authors": [
        {
            "name": "TeaRiot",
            "email": "teaRiot@yandex.ru"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "TeaRiot\\YandexGpt\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
